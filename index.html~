<!DOCTYPE html>
<head>
<script src="system/init/init.js"></script>
</head>
<body>
<p id="show"></p>
<script type="text/javascript">
document.getElementById("show").innerHTML = "none";
if(init()) {
	registerCommand("show",function (event) {
		if(event.data.data != undefined) {
			document.getElementById("show").innerHTML = event.data.data;
		} else {
			document.getElementById("show").innerHTML = event.data;
		}
	});
	startProcess("apps/count/count.js");
}

</script>



</body>